#ifndef USERLIB_INC_LED_H_
#define USERLIB_INC_LED_H_

#include "stm32f1xx_hal.h"

typedef struct LED_t {
	uint16_t port;
	uint pin;
}LED;


typedef enum color_enum{
	RED = 0,
	GREEN,
	YELLOW,
	BLUE
}color;


